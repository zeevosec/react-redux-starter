{
  "name": "react-redux-starter",
  "title": "React Redux Starter",
  "version": "2020.10.0",
  "description": "Starter project with React and Redux with Redux Saga",
  "keywords": [
    "React",
    "Redux",
    "Redux-Saga",
    "Webpack",
    "ES6"
  ],
  "bugs": "https://github.com/zeevosec/react-redux-saga-starter/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeevosec/react-redux-saga-starter"
  },
  "license": "MIT",
  "author": "Shane O'Neill <oneill.shane.h@gmail.com>",
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/webpack.config.js --profile --json > webpack.stats.json",
    "build": "node tools/build",
    "build:serve": "npm run build && npm run serve",
    "deploy": "node tools/deploy",
    "format": "prettier \"**/*.{js,jsx}\" --write",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint  --ext .js --ext .jsx src",
    "lint:styles": "stylelint 'src/**/*.{js,jsx}'",
    "publish": "node tools/publish",
    "serve": "serve --listen 3000 build",
    "size": "npm run build && size-limit",
    "start": "node tools/start",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --bail --coverage",
    "test:watch": "jest --watch --verbose",
    "validate": "npm run lint && npm run lint:styles && npm run test:coverage && flow && npm run build && npm run test:e2e && size-limit"
  },
  "lint-staged": {
    "*.(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write",
      "git add"
    ],
    "*.js?(x)": [
      "eslint --fix",
      "git add",
      "jest --findRelatedTests"
    ]
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
